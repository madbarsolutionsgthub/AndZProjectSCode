<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.j.nike">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/nike_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityMen"
            android:label="@string/title_activity_men"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityWomen"
            android:label="@string/title_activity_women"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityBoys"
            android:label="@string/title_activity_boys"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityGirls"
            android:label="@string/title_activity_girls"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Web"
            android:theme="@style/WebTheme"></activity>
    </application>

</manifest>